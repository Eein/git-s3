#!/usr/bin/env php
<?php

// unset time limit since the commands may take a while
set_time_limit(0);

require __DIR__ . '/../vendor/autoload.php';


// load config
$config = new GitS3\Wrapper\Config(__DIR__ . '/../config.yml');

// start application
$app = new GitS3\Console\Application($config);
$app->run();