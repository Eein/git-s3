{
	"name": "schickling/git-s3",
	"description": "Easy way to deploy your git repo to a AWS S3 bucket",
	"keywords": ["git", "s3", "aws", "deploy", "bucket"],
	"require": {
		"symfony/console": "2.4.*@dev",
		"symfony/yaml": "2.4.*@dev",
		"symfony/finder": "2.4.*@dev",
		"symfony/filesystem": "2.4.*@dev",
		"aws/aws-sdk-php": "2.4.*@dev",
		"cpliakas/git-wrapper": "dev-master"
	},
    "require-dev": {
        "mockery/mockery": "dev-master"
    },
	"autoload": {
		"psr-0": {
			"GitS3": "src/"
		}
	},
	"minimum-stability": "dev"
}